<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="InterviewInfo">
        <attr name="iconDrawable" format="reference|integer" />
        <attr name="text" format="string" />
    </declare-styleable>
    <attr name="divider_width" format="dimension">
        <enum name="fill_parent" value="-1" />
        <enum name="match_parent" value="-1" />
        <enum name="wrap_content" value="-2" />
    </attr>
    <attr name="divider_height" format="dimension">
        <enum name="fill_parent" value="-1" />
        <enum name="match_parent" value="-1" />
        <enum name="wrap_content" value="-2" />
    </attr>
    <attr name="divider_color" format="color"/>
    <attr name="item_color" format="reference|color"/>
    <attr name="item_number" format="reference|string"/>

    <declare-styleable name="RankAppItemView">
        <attr name="rank_number" format="reference|string" />
        <attr name="rank_color" format="reference|color" />
        <attr name="rank_title" format="reference|string" />
        <attr name="rank_description" format="reference|string" />
        <attr name="rank_appIconDrawable" format="reference|integer" />
        <attr name="rank_appIconVisibility" format="reference|integer">
            <enum name="gone" value="8" />
            <enum name="invisible" value="4" />
            <enum name="visible" value="0" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RankItemView">
        <attr name="rank_item_line_height" format="reference|dimension"/>
        <attr name="rank_item_color" format="reference|color" />
        <attr name="rank_item_title" format="reference|string" />
        <attr name="rank_item_description" format="reference|string" />
        <attr name="rank_item_content_isTop" format="reference|boolean"/>
    </declare-styleable>

</resources>